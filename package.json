{
  "name": "express-restful-api",
  "version": "1.0.0",
  "description": "A boilerplate for building RESTful api with Express.js",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon src/index.js",
    "start:prod": "cross-env NODE_ENV=production node src/index.js",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "bail": true,
    "verbose": true,
    "collectCoverage": false
  },
  "keywords": [
    "Javascript",
    "Express.js",
    "RESTful API"
  ],
  "author": "evanalifian",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.15.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "prisma": "^5.15.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "@types/express": "^4.17.21",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "supertest": "^7.0.0"
  }
}
